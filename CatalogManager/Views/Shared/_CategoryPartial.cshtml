@model CatalogManager.Models.Category


<div class="row category">
    <h2>@Model.Name</h2>
    @Html.ActionLink("View", "Index", "Category", new { name = @Model.Name }, null)
    @Html.ActionLink("Edit", "Edit", "Category", new { name = @Model.Name, @pageType = @ViewData["PageType"] }, null)
    @Html.ActionLink("Delete", "Delete", "Category", new { name = @Model.Name, @pageType = @ViewData["PageType"] }, null)
</div>

